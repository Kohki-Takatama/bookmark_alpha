{
  "manifest_version": 3,
  "name": "Bookmark Search Extension",
  "version": "1.0",
  "permissions": ["bookmarks", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space"
      }
    }
  },
  "action": {
    "default_popup": "popup.html"
  }
}
