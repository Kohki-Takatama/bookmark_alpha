{
  "manifest_version": 3,
  "name": "ブックマーク検索・ショートカット",
  "version": "1.0",
  "permissions": ["bookmarks", "activeTab", "tabs", "tabGroups"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "MacCtrl+Space"
      }
    }
  },
  "action": {
    "default_popup": "popup.html"
  }
}
